<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--填写UserMapper位置-->
<mapper namespace="com.example.demo1.Stores.StoreMapper">
    <select id="findAll" resultType="com.example.demo1.Stores.Store">
        SELECT * FROM  stores
    </select>
    <select id="findNumber" resultType="Integer">
        SELECT number FROM  stores
    </select>
    <select id="findYear" resultType="Integer">
        SELECT year FROM  stores
    </select>
</mapper>